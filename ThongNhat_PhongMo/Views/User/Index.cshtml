@model IEnumerable<ThongNhat_PhongMo.Models.User>

@{
    ViewData["Title"] = "Thông tin nhận viên";
    Layout = "_LayoutUser";
}
<p>
    <a asp-action="Create">Tạo mới</a>
</p>
<div class="col-md-12">
    <table class="table">
        <thead>
            <tr>
                <th>
                    Tài khoản
                </th>
                <th>
                    Họ tên
                </th>

                <th></th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.UserName)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.hoten)
                    </td>
                    <td>
                        <a asp-action="Edit" asp-route-id="@item.Id">Chỉnh sửa</a> @* | *@
                        @* <a asp-action="Details" asp-route-id="@item.id">Details</a> | *@
                        @* <a asp-action="Delete" asp-route-id="@item.id">Xóa</a> *@
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>
